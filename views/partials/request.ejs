<nav>
  <div id='title'>Ribbid</div>
  <ul id='left-btns' class='btn-list'>
    <li><a href="#" onclick="window.history.back();"><i class="fa fa-chevron-left"></i></a></li>
  </ul>
</nav>
<div id="content-wrap">
  <div class="large-view">
    <div class="picture">
      <img src={{request.user.profilePic}} />
    </div>
    <div class="float-group">
      <div class="name">{{request.user.displayName}}</div>
      <div class="title">{{request.title}}</div>
    </div>
    <div class="clearfix"></div>
    <div class="description">{{request.description}}</div>
  </div>
  <map zoom="14" style="width: 100%; height: 400px;"></map>
	<ul class="list-view" id="request-page">
    <li ng-hide="making_bid" ng-click="startBid()" class="list-item text-item"><a href="#"><i class="fa fa-plus"> Make a Bid</i></a></li>
    <li ng-class="{making_bid: making_bid}" ng-show="making_bid" class="list-item text-item"><form ng-submit="makeBid(bid)"><input placeholder="Enter a Bid" ng-blur="cancelBid()" ng-model="bid" id="bidAmnt"></input></form></li>
    <li class="list-item" ng-repeat="a in [1,2,3,4,5,6,7,8,9]">
      <div class="item-wrap">
        <div class="picture">
          <img src="https://scontent-a-lga.xx.fbcdn.net/hphotos-xpa1/v/t1.0-9/10922822_396217957207564_6547384044584887570_n.jpg?oh=5bb3bf03ae730fb0266e298716eef2a7&oe=55671980" />
        </div>
        <div class="float-group">
          <div class="name">Austin Eng</div>
          <div class="bid">$9.30<span class="timestamp">1/5/13 5:34 PM</span></div>
        </div>
      </div>
    </li>
  </ul>
</div>